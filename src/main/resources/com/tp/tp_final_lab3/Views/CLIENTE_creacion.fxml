      </Text>
      <TextField fx:id="textNombre" layoutX="58.0" layoutY="78.0" promptText="Nombre" />
      <TextField fx:id="textApellido" layoutX="58.0" layoutY="109.0" promptText="Apellido" />
      <TextField fx:id="textDNI" layoutX="58.0" layoutY="140.0" promptText="DNI" />
      <TextField fx:id="textCUIT" layoutX="58.0" layoutY="171.0" promptText="CUIT" />
